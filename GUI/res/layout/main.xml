<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<LinearLayout android:layout_width="fill_parent"
		android:id="@+id/linearLayout1" android:layout_height="wrap_content">
		<Spinner android:layout_height="wrap_content" android:id="@+id/mainSpinner"
			android:layout_weight="1" android:layout_width="wrap_content"></Spinner>
		<AutoCompleteTextView android:layout_height="wrap_content"
			android:id="@+id/searchAutoCompleteTextView" android:hint="@string/searchHint"
			android:layout_weight="1" android:layout_width="wrap_content"></AutoCompleteTextView>
		<ImageView android:layout_gravity="center"
			android:layout_height="fill_parent" android:src="@drawable/searchicon"
			android:id="@+id/imageView1" android:scaleType="fitXY"
			android:layout_width="50sp"></ImageView>
	</LinearLayout>
</LinearLayout>
